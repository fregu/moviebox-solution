{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "plugin:react/recommended",
    "prettier",
    "prettier/react",
    "prettier/standard",
    "plugin:flowtype/recommended",
    "prettier/flowtype",
    "plugin:import/errors"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["jsx-a11y", "react", "prettier", "standard", "flowtype"]
}
